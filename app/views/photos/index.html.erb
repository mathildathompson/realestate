<h3>Photo Albums</h3>

<%#= photo.property.address_number %> <%#= property.address_street%>, 
        <%#= property.address_suburb %>, <%#= property.address_code %>


<% @photos.each do |photo| %>
    <p>Property ID: <%= link_to(photo.property_id, property_path(photo.property_id)) %></p>
    <ul>
        <li><%= link_to(image_tag(photo.photo1), photo, :class => 'thumb') %></li>
        <li><%= link_to(image_tag(photo.photo2), photo, :class => 'thumb') %></li>
        <li><%= link_to(image_tag(photo.photo3), photo, :class => 'thumb') %></li>
        <li><%= link_to(image_tag(photo.photo4), photo, :class => 'thumb') %></li>
        <li><%= link_to(image_tag(photo.photo5), photo, :class => 'thumb') %></li>
        <li><%= link_to(image_tag(photo.photo6), photo, :class => 'thumb') %></li>
        <li><%= link_to(image_tag(photo.photo7), photo, :class => 'thumb') %></li>
        <li><%= link_to(image_tag(photo.photo8), photo, :class => 'thumb') %></li>
    </ul>
<% end %>
    
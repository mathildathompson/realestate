<section class="embed-wrapper embed-sm">
    <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d3313.0840077487505!2d151.2093257!3d-33.86172579999999!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x6b12ae426582c3ab%3A0x70d1b9ea50b3a5ef!2s1+Pitt+St!5e0!3m2!1sen!2s!4v1395716824104" width="600" height="450" frameborder="0" style="border:0"></iframe>
</section><!-- /.embed-wrapper -->

    <section class="wrapper-md">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 col-md-4 col-md-offset-1">
                    <h3>Contact data</h3>
                    <div class="well">
                        <p> 1 Pitt Street, Sydney, 2000</p>
                        <p>+61 (2) 9784-8000</p>
                        <p>coinhomeaus@gmail.com</p>
                        <hr>
                    </div><!-- /.well -->
                </div><!-- /.col -->
                <div class="col-sm-12 col-md-6">
                    <h3>Contact by email</h3>
                    <%= form_tag('/pages/contact') do %>
                        <div class="form-group">
                            <%= email_field_tag :email, nil, :placeholder => 'Enter email', :class => 'form-control' %> 
                        </div>
                        <div class="form-group">
                            <div class="form-group">
                            <%= label_tag :message %>
                            <%= text_area_tag :message, nil, :placeholder => 'Enter message', :class => 'form-control', :rows => '4' %>
                        </div>
                        <%= submit_tag 'Send Message', class: 'btn btn-primary' %>
                    <% end %>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container -->
    </section>
